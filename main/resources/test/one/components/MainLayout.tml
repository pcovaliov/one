<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
	<head>
		<title>My web page</title>
	</head>
	<body>
		<header id="mainlayoutheader">
			<t:PageLink t:page="Index">${message:home}</t:PageLink>
			<t:PageLink t:page="About">${message:about}</t:PageLink>
			<t:PageLink t:page="Contact">${message:contact}</t:PageLink>
		</header>
		<section>
			<t:body/>
		</section>
		<footer id="mainlayoutfooter">
			<t:PageLink t:page="Index">${message:home}</t:PageLink>
			<t:PageLink t:page="About">${message:about}</t:PageLink>
			<t:PageLink t:page="Contact">${message:contact}</t:PageLink>
		</footer>
	</body>
</html>